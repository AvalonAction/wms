<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="basis" namespace="/basis" extends="wms-default">
	
		
		<!--仓库 -->
		<action name="warehouse" class="WarehouseAction">
			<result name="success">/WEB-INF/view/basis/warehouseList.jsp</result>
			<result name="input">/WEB-INF/view/basis/warehouseEdit.jsp</result>
		</action>
		
		<!--供应商 -->
		<action name="supplier" class="SupplierAction">
			<result name="success">/WEB-INF/view/basis/supplierList.jsp</result>
			<result name="input">/WEB-INF/view/basis/supplierEdit.jsp</result>
		</action>
		
		<!--配送方式 -->
		<action name="shipping" class="ShippingAction">
			<result name="success">/WEB-INF/view/basis/shippingList.jsp</result>
			<result name="input">/WEB-INF/view/basis/shippingEdit.jsp</result>
		</action>
		
		<!--打印模板 -->
		<action name="printableTemplate" class="PrintableTemplateAction">
			<result name="success">/WEB-INF/view/basis/printableTemplate.jsp</result>
			<result name="input_shopping_list">/WEB-INF/view/basis/shoppingListTemplateEdit.jsp</result>
			<result name="input_shipping">/WEB-INF/view/basis/shippingTemplateEdit.jsp</result>
		</action>
		
		<!--权限 -->
		<action name="authorization" class="AuthorizationAction">
			<result name="success">/WEB-INF/view/basis/authorizationList.jsp</result>
			<result name="input">/WEB-INF/view/basis/authorizationEdit.jsp</result>
		</action>
		
		<!-- 视图 -->

	</package>
</struts>